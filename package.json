{
  "version": "0.24.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "prepare": "npm run build",
    "clean:dist": "rimraf dist/",
    "build:types": "tsc src/index.ts --declarationDir dist/ --lib esnext --emitDeclarationOnly --declaration",
    "build:lib": "vite build",
    "build": "npm run clean:dist && npm run build:lib && npm run build:types",
    "generate": "node ./scripts/generate.js",
    "check-missing": "npm run generate && npm run build && node ./scripts/check-missing.js",
    "format-data": "prettier --loglevel warn --write \"src/data/**/*.json\"",
    "prepublishOnly": "npm run generate && npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "genshin-data",
  "author": "dvaJi",
  "devDependencies": {
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7"
  }
}
