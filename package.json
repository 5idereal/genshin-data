{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "generate": "ts-node ./scripts/generate.ts",
    "check-missing": "ts-node ./scripts/check-missing.ts",
    "format-data": "prettier --write \"data/**/*.json\"",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "npm run generate && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "genshin-data",
  "author": "dvaJi",
  "module": "dist/.esm.js",
  "size-limit": [
    {
      "path": "dist/.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "chalk": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
